<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>java_web</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>


  <servlet>
  	<servlet-name>loginServlet</servlet-name>
  	<servlet-class>com.mk.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>loginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>goodsManage</servlet-name>
  	<servlet-class>com.mk.servlet.GoodsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>goodsManage</servlet-name>
  	<url-pattern>/goodsManage</url-pattern>
  </servlet-mapping>
</web-app>
